  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "mongoose": "^7.0.3",
    "cookie-parser": "^1.4.6",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "dotenv": "^16.4.5",
    "nodemon": "^2.0.21"
  }
}
